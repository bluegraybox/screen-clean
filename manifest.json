{
	"manifest_version": 3,
	"name": "Screen Clean",
	"version": "1.0",
	"permissions": [
		"activeTab"
	],
	"content_scripts": [
		{
			"matches": ["https://*.bandsintown.com/*"],
			"run_at": "document_end",
			"css": ["bandsintown.css"],
			"js": ["bandsintown.js"]
		},
		{
			"matches": ["https://*.bbc.com/*"],
			"run_at": "document_end",
			"css": ["bbc.css"]
		},
		{
			"matches": ["https://*.bloomberg.com/*"],
			"run_at": "document_end",
			"css": ["bloomberg.css"]
		},
		{
			"matches": ["https://*.buzzfeednews.com/*"],
			"run_at": "document_end",
			"css": ["buzzfeednews.css"]
		},
		{
			"matches": ["https://*.citylab.com/*"],
			"run_at": "document_end",
			"css": ["citylab.css"]
		},
		{
			"matches": ["https://*.economist.com/*"],
			"run_at": "document_end",
			"css": ["economist.css"]
		},
		{
			"matches": ["https://*.facebook.com/*"],
			"run_at": "document_end",
			"css": ["facebook.css"],
			"js": ["facebook.js"],
			"all_frames": true
		},
		{
			"matches": ["https://*.fastcompany.com/*"],
			"run_at": "document_end",
			"css": ["fastcompany.css"]
		},
		{
			"matches": ["https://*.theguardian.com/*", "https://*.guardian.co.uk/*"],
			"run_at": "document_end",
			"css": ["guardian.css"]
		},
		{
			"matches": ["https://*.instagram.com/*"],
			"run_at": "document_end",
			"css": ["instagram.css"],
			"js": ["instagram.js"]
		},
		{
			"matches": ["https://*.investopedia.com/*"],
			"run_at": "document_end",
			"css": ["investopedia.css"]
		},
		{
			"matches": ["https://*.linkedin.com/*"],
			"run_at": "document_end",
			"css": ["linkedin.css"]
		},
		{
			"matches": ["https://*.mewe.com/*"],
			"run_at": "document_end",
			"css": ["mewe.css"],
			"js": ["mewe.js"]
		},
		{
			"matches": ["https://*.nextdoor.com/*"],
			"run_at": "document_end",
			"css": ["nextdoor.css"]
		},
		{
			"matches": ["https://*.nytimes.com/*"],
			"run_at": "document_end",
			"css": ["nytimes.css"]
		},
		{
			"matches": ["https://*.questionablecontent.net/*"],
			"run_at": "document_end",
			"css": ["questionablecontent.css"],
			"js": ["questionablecontent.js"]
		},
		{
			"matches": ["https://*.reactormag.com/*"],
			"run_at": "document_end",
			"css": ["reactormag.css"]
		},
		{
			"matches": ["https://*.slate.com/*"],
			"run_at": "document_end",
			"css": ["slate.css"]
		},
		{
			"matches": ["https://*.theatlantic.com/*"],
			"run_at": "document_end",
			"css": ["theatlantic.css"]
		},
		{
			"matches": ["https://*.tumblr.com/*"],
			"run_at": "document_end",
			"css": ["tumblr.css"]
		},
		{
			"matches": ["https://*.wired.com/*"],
			"run_at": "document_end",
			"css": ["wired.css"]
		},
		{
			"matches": ["https://*.washingtoncitypaper.com/*"],
			"run_at": "document_end",
			"css": ["washingtoncitypaper.css"]
		},
		{
			"matches": ["https://*.washingtonpost.com/*"],
			"run_at": "document_end",
			"css": ["washingtonpost.css"]
		},
		{
			"matches": ["https://*.youtube.com/*"],
			"run_at": "document_end",
			"css": ["youtube.css"],
			"js": ["youtube.js"]
		}
	]
}
